    "express": "~4.16.0",
    "express-validator": "^5.3.1",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jimp": "^0.6.0",
    "mongoose": "^5.4.13",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "nodemon": "^1.14.11",
    "passport": "^0.4.0",
